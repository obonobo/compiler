	// [..., FINAL_FUNC_DEF]
	// 		Then create a new list containing the top
	SEM_REPT_PROG0_MAKESIBLING: func(action Kind, tok Token, semanticStack *[]*ASTNode) {
		l := len(*semanticStack)
		if l == 0 {
			panic("Expected semantic stack to have structOrImplOrFuncList siblings on top")
		}

		top := (*semanticStack)[l-1]

		// Type check
		switch top.Type {
		case FINAL_FUNC_DEF, FINAL_IMPL_DEF, FINAL_STRUCT_DECL:
		default:
			panic(fmt.Errorf("expected stack top to be one of %v %v or %v",
				FINAL_FUNC_DEF, FINAL_IMPL_DEF, FINAL_STRUCT_DECL))
		}

		// Pop the top off
		*semanticStack = (*semanticStack)[:l-1]

		// Is there already a list? Check the 2nd to top
		if l >= 2 {
			secondTop := (*semanticStack)[l-2]
			if secondTop.Type == FINAL_STRUCT_OR_IMPL_OR_FUNC_LIST {
				secondTop.Children = append(secondTop.Children, top)
				// secondTop.Children = append([]*ASTNode{top}, secondTop.Children...)
				return
			}
		}

		// Otherwise, make a new list
		node := &ASTNode{
			Type:     FINAL_STRUCT_OR_IMPL_OR_FUNC_LIST,
			Children: []*ASTNode{top},
		}
		*semanticStack = append(*semanticStack, node)
	},

