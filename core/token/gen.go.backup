type Rule struct {
	LHS Kind   // The Left Hand Side nonterminal symbol for this rule
	RHS []Kind // The RHS sentential form for this rule
}

func (r Rule) String() string {
	var rhs string
	for _, r := range r.RHS {
		rhs += string(r)
	}
	return fmt.Sprintf("%v ::= %v", r.LHS, rhs)
}

